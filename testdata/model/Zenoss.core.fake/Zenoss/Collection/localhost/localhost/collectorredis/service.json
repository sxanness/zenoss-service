{
    "CPUCommitment": 1,
    "Command": "/usr/bin/python3 -m http.server 8080",
    "Description": "Redis server",
    "Endpoints": [
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "{{(parent .).Name}}_redis",
            "Name": "CollectorRedis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "export"
        }
    ],
    "HealthChecks": {
        "metrics": {
            "Interval": 15,
            "Script": "echo OK"
        }
    },
    "ImageID": "zenoss/ubuntu:wget",
    "Instances": {
        "max": 1,
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "path": "/var/log/redis/redis.log",
            "type": "redis"
        }
    ],
    "MonitoringProfile": {
        "GraphConfigs": [
            {
                "datapoints": [
                    {
                        "aggregator": "sum",
                        "fill": false,
                        "format": "%d",
                        "id": "metricsLength",
                        "legend": "Metrics",
                        "metric": "redis.metricsLength",
                        "metricSource": "redis",
                        "name": "Metric Queue Length",
                        "rate": false,
                        "type": "line"
                    }
                ],
                "description": "Number of metrics awaiting processing",
                "footer": false,
                "id": "metricqueue",
                "name": "Metric Queue Length",
                "range": {
                    "end": "0s-ago",
                    "start": "1h-ago"
                },
                "returnset": "EXACT",
                "type": "line",
                "yAxisLabel": "Metrics"
            },
            {
                "datapoints": [
                    {
                        "aggregator": "avg",
                        "fill": false,
                        "format": "%d",
                        "id": "total_commands_processed",
                        "legend": "Data Points",
                        "metric": "redis.total_commands_processed",
                        "metricSource": "redis",
                        "name": "Total Commands",
                        "rate": true,
                        "type": "line"
                    }
                ],
                "description": "Number of commands per second",
                "footer": false,
                "id": "totalcommands",
                "name": "Total Commands",
                "range": {
                    "end": "0s-ago",
                    "start": "1h-ago"
                },
                "returnset": "EXACT",
                "type": "line",
                "yAxisLabel": "Commands/s"
            }
        ],
        "MetricConfigs": [
            {
                "Description": "redis internal metrics",
                "ID": "redis",
                "Metrics": [
                    {
                        "Counter": true,
                        "Description": "Number of commands processed by redis.",
                        "ID": "redis.total_commands_processed",
                        "Name": "Total Commands",
                        "Unit": "Count"
                    },
                    {
                        "Counter": false,
                        "Description": "Number of metrics awaiting processing",
                        "ID": "redis.metricsLength",
                        "Name": "Metrics Queue Length",
                        "Unit": "Count"
                    }
                ],
                "Name": "redis internal metrics"
            }
        ]
    },
    "Name": "redis",
    "RAMCommitment": "1G",
    "Tags": [
        "daemon",
        "collector"
    ]
}
