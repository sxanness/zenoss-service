{
    "CPUCommitment": 1,
    "Command": "/usr/bin/python3 -m http.server 8080",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zauth/zauth-zope.conf": {
            "FileName": "/opt/zenoss/etc/zauth/zauth-zope.conf",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zodb_db_main.conf": {
            "FileName": "/opt/zenoss/etc/zodb_db_main.conf",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zodb_db_session.conf": {
            "FileName": "/opt/zenoss/etc/zodb_db_session.conf",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zope.conf": {
            "FileName": "/opt/zenoss/etc/zope.conf",
            "Permissions": "0664"
        }
    },
    "Description": "Manages authentication tokens for RM",
    "Endpoints": [
        {
            "Application": "zauth",
            "Name": "zauth",
            "PortNumber": 9180,
            "Protocol": "tcp",
            "Purpose": "export"
        },
        {
            "Application": "zodb_.*",
            "Name": "mariadb",
            "Purpose": "import"
        },
        {
            "Application": "memcached",
            "Name": "memcached",
            "PortNumber": 11211,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "memcached-session",
            "Name": "memcached-session",
            "PortNumber": 11212,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "rabbitmq",
            "Name": "rabbitmq",
            "PortNumber": 5672,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "HealthChecks": {
        "answering": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "mariadb_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "memcached_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "memcached_session_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "rabbit_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "running": {
            "Interval": 5.0,
            "Script": "echo OK"
        }
    },
    "ImageID": "zenoss/ubuntu:wget",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "zope",
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/event.log",
            "type": "zauth_eventlog"
        },
        {
            "filters": [
                "z2_access_logs"
            ],
            "path": "/opt/zenoss/log/Z2.log",
            "type": "zauth_access_logs"
        }
    ],
    "Name": "zauth",
    "Prereqs": [],
    "RAMCommitment": "1G",
    "Tags": [
        "daemon"
    ]
}
