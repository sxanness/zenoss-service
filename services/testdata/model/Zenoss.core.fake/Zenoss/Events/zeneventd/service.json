{
    "Actions": {
        "debug": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} debug'",
        "stats": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} stats'"
    },
    "CPUCommitment": 1,
    "Command": "/usr/bin/python3 -m http.server 8080",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zeneventd.conf": {
            "FileName": "/opt/zenoss/etc/zeneventd.conf",
            "Permissions": "0664"
        }
    },
    "Description": "Performs event mappings, transformations, and other data-driven event processing tasks",
    "Endpoints": [
        {
            "Application": "zodb_.*",
            "Name": "mariadb",
            "Purpose": "import"
        },
        {
            "Application": "memcached",
            "Name": "memcached",
            "PortNumber": 11211,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "rabbitmq.*",
            "Name": "rabbitmq",
            "Purpose": "import"
        },
        {
            "Application": "zep",
            "Name": "zep",
            "PortNumber": 8084,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "HealthChecks": {
        "mariadb_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "memcached_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "rabbit_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "redis_answering": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "running": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "stable_workers": {
            "Interval": 5.0,
            "Script": "echo OK"
        }
    },
    "ImageID": "zenoss/ubuntu:wget",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/zeneventd.log",
            "type": "zeneventd"
        }
    ],
    "Name": "zeneventd",
    "Prereqs": [
        {
            "Name": "MariaDB connectivity",
            "Script": "su - zenoss -c '/opt/zenoss/bin/python /opt/zenoss/Products/ZenUtils/ZenDB.py --usedb zodb --execsql=\";\"'"
        }
    ],
    "RAMCommitment": "1G",
    "Services": [],
    "Tags": [
        "daemon"
    ]
}
